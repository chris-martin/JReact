<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="White box">

  <test name="Pipe looping">
    <classes>
      <class name="org.jreact.core.impl.PipeDependentEffectsTest"/>
      <class name="org.jreact.core.impl.PipeDependentSinksTest"/>
    </classes>
  </test>

  <test name="Pipe limitations">
    <classes>
      <class name="org.jreact.core.PipeLimitTimesTest"/>
      <class name="org.jreact.core.PipeLimitStreamTest"/>
      <class name="org.jreact.core.impl.RelayingPipeLimitedTimesTest"/>
    </classes>
  </test>

</suite>
